SELECT 
  *,
  IFNULL(CAST(ProductionYear AS STRING), 'Unknown') AS YearLabel,
  CASE 
    WHEN ProductionYear IS NOT NULL THEN CAST(FLOOR(ProductionYear / 10) * 10 AS STRING) || 's'
    ELSE 'Unknown'
  END AS Decade,
  SAFE_DIVIDE(
    (CAST(SPLIT(RunningTime, ':')[SAFE_OFFSET(0)] AS INT64) * 60) + 
     SAFE_CAST(SPLIT(RunningTime, ':')[SAFE_OFFSET(1)] AS INT64), 
    1
  ) AS TotalMinutes,
  CASE 
    WHEN Rating >= 9 THEN '9 to 10'
    WHEN Rating >= 8 THEN '8 to 9'
    WHEN Rating >= 7 THEN '7 to 8'
    WHEN Rating >= 6 THEN '6 to 7'
    WHEN Rating >= 5 THEN '5 to 6'
    ELSE 'Other/No Rating'
  END AS RatingRange
FROM `mrdipeshashah.mydvdcollection.dvdcollection` 
WHERE OriginalTitle IS NOT NULL;
